sub pgMain_ShotClockSecondChanged (Sender, Hours, Minutes, Seconds, Miliseconds)
    if Seconds <= 10 then
        pgMain.ShotClock.Font.Color = clRed
    else
        pgMain.ShotClock.Font.Color = clSilver
    end if
end sub

